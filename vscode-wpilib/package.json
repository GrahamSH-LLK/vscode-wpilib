{
    "name": "vscode-wpilib",
    "displayName": "WPILib",
    "author": "WPILib Suite",
    "description": "VSCode extension for WPILib Team Use (Not supported from VS Code Extension Marketplace)",
    "version": "2023.0.0",
    "publisher": "wpilibsuite",
    "license": "BSD-3-Clause",
    "icon": "wpilib-128.png",
    "engines": {
        "vscode": "^1.57.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:.wpilib/wpilib_preferences.json",
        "workspaceContains:build/vscodeconfig.json"
    ],
    "main": "./out/extension",
    "repository": "https://github.com/wpilibsuite/vscode-wpilib",
    "homepage": "https://github.com/wpilibsuite/vscode-wpilib",
    "scripts": {
        "vscode:prepublish": "gulp build && webpack --mode production",
        "clean": "rmdir-cli out",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "webpack": "webpack --mode development",
        "webpack-watch": "webpack --mode development --watch",
        "unittest": "mocha -r ts-node/register -u tdd src/unittest/*.ts",
        "gulp": "gulp",
        "test": "npm run webpack && node ./node_modules/vscode/bin/test",
        "updateActivation": "gulp update-activation",
        "vscePackage": "vsce package"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/micromatch": "^4.0.1",
        "@types/mkdirp": "^1.0.1",
        "@types/mocha": "^8.0.3",
        "@types/ncp": "^2.0.4",
        "@types/node": "^10.2.0",
        "@types/node-fetch": "^2.5.7",
        "@types/triple-beam": "^1.3.1",
        "@types/vscode": "1.57.0",
        "@types/xml2js": "^0.4.5",
        "@vscode/vsce": "^2.20.1",
        "copy-webpack-plugin": "^6.1.1",
        "del": "^6.0.0",
        "event-stream": "^4.0.1",
        "gulp": "^4.0.2",
        "gulp-filter": "^6.0.0",
        "gulp-json-transform": "^0.4.7",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-typescript": "^5.0.1",
        "gulp-yaml": "^2.0.4",
        "mocha": "^10.2.0",
        "rmdir-cli": "^2.0.6",
        "run-sequence": "^2.2.1",
        "ts-loader": "^8.4.0",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "typescript": "^3.9.5",
        "vscode-nls-dev": "^4.0.4",
        "webpack": "4.47.0",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "extract-zip": "^2.0.1",
        "glob": "^7.1.6",
        "java-properties": "^1.0.2",
        "jsonc-parser": "^2.3.1",
        "logform": "^2.2.0",
        "micromatch": "^4.0.2",
        "mkdirp": "^1.0.4",
        "ncp": "^2.0.0",
        "node-fetch": "^2.6.1",
        "triple-beam": "^1.3.0",
        "vscode-cpptools": "^6.1.0",
        "vscode-jsonrpc": "^8.1.0",
        "vscode-nls": "^5.2.0",
        "vscode-wpilibapi": "^2021.1.1",
        "winston": "^3.3.3",
        "winston-transport": "^4.4.0",
        "wpilib-riolog": "^0.2.0",
        "xml2js": "^0.6.2"
    },
    "overrides": {
        "webpack@4.47.0": {
            "terser-webpack-plugin": "^4.2.3"
        }
    }
}